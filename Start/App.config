<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application>
      <channels>
        <channel ref="tcp" port="13003" >
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
      <service>
        <wellknown mode="Singleton" type="RemoteEventInterface, RemoteEventInterface" objectUri="RemoteEventInterface" />
      </service>
    </application>
  </system.runtime.remoting>
</configuration>
